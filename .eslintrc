{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "linebreak-style": ["error", "windows"],
        "require-jsdoc": "off",
        "indent": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "object-curly-spacing": "off",
        "quotes": "off",
        "arrow-parens": "off",
        "operator-linebreak": "off"
    },
    "extends": ["eslint:recommended", "google"]
}